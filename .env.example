# Semantext Hub docker-compose environment
# Copy to .env and customize before running `docker compose up`

NODE_ENV=development
HUB_PORT=3000
REDIS_PORT=6379
REGISTRY_API_KEY=replace-with-secure-value
REGISTRY_DB_PATH=/app/var/registry.sqlite
LANCEDB_PATH=/app/data/lancedb
REDIS_URL=redis://redis:6379/0
MCP_METRICS_LOG_MODE=file
MCP_METRICS_LOG_INTERVAL_MS=300000
