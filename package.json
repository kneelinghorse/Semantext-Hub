{
  "name": "oss-protocols",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "description": "Protocol-Driven Discovery tooling and missions",
  "bin": {
    "sch": "./cli/index.js",
    "sch-cli": "./cli/index.js",
    "protocol-discover": "./bin/deprecated-protocol-discover.js",
    "protocol-generate": "./bin/deprecated-protocol-generate.js",
    "protocol-mcp-server": "./packages/runtime/bin/protocol-mcp-server.js"
  },
  "scripts": {
    "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js",
    "test:watch": "jest --watch",
    "test:ci": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --ci --coverage --selectProjects core --maxWorkers=2 --testTimeout=30000 && node --experimental-vm-modules ./node_modules/jest/bin/jest.js --ci --selectProjects catalog-cli wsap-cli --maxWorkers=2 --testTimeout=30000",
    "test:fast": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --config ./jest.fast.config.js --ci --maxWorkers=2 --testTimeout=20000",
    "generate:sprint-summary": "tsx ./scripts/reports/sprint-summary-generator.ts",
    "test:esm:probe": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js -c ./jest.esm.ci.js --runTestsByPath tests/_probe/esm.sample.test.ts",
    "test:performance": "node packages/runtime/scripts/performance-benchmark.js",
    "test:deflake": "node tests/util/deflake-runner.js",
    "test:deflake:report": "node tests/util/deflake-reporter.js",
    "check:boundaries": "node scripts/check-layer-boundaries.js",
    "check:cli-duplicates": "./scripts/ci/check-duplicate-commands.sh",
    "perf:report": "node cli/index.js perf report",
    "perf:status": "node cli/index.js perf status",
    "demo:preflight": "node scripts/preflight/demo-preflight.mjs",
    "release:bundle": "node scripts/release/create-launch-bundle.mjs",
    "cli": "node ./cli/index.js",
    "wsap": "node app/cli/wsap.mjs",
    "build": "echo 'No build step required for Node.js'",
    "start": "node packages/runtime/bin/protocol-mcp-server.js",
    "docker:build": "docker build -t ossp-agi-mcp:latest .",
    "docker:run": "docker run -d --name ossp-agi-mcp -e PROTOCOL_ROOT=/app ossp-agi-mcp:latest",
    "docker:stop": "docker stop ossp-agi-mcp && docker rm ossp-agi-mcp",
    "ci:full": "npm run demo:preflight -- --ci --json && npm run check:cli-duplicates && npm run check:boundaries && npm run ci:check-registry && npm run test:ci && npm run test:performance && npm run docker:build",
    "probe:pre12": "node scripts/probe/pre12.collect.js all",
    "test:e2e:mcp": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --runTestsByPath tests/e2e/mcp.e2e.test.ts",
    "test:registry:parity": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --runTestsByPath tests/runtime/registry.http.parity.spec.mjs",
    "ci:check-registry": "node scripts/ci/check-registry-single-entry.mjs",
    "verify:coverage": "node scripts/ci/verify-coverage-thresholds.mjs"
  },
  "devDependencies": {
    "@apidevtools/swagger-parser": "^10.0.3",
    "@asyncapi/parser": "^3.4.0",
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "light-my-request": "^6.6.0",
    "node-fetch": "^3.3.2",
    "playwright": "^1.56.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "supertest": "^6.3.4",
    "tsx": "^4.15.7",
    "yaml": "^2.8.1"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "chalk": "^5.6.2",
    "cli-table3": "^0.6.5",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "eventemitter3": "^5.0.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "fs-extra": "^11.3.2",
    "graphology": "^0.26.0",
    "graphology-traversal": "^0.3.1",
    "inquirer": "^12.9.6",
    "lru-cache": "^10.4.3",
    "ora": "^7.0.1",
    "p-retry": "^5.1.2",
    "pg": "^8.16.3",
    "xmlbuilder2": "^3.1.1",
    "xxhash-addon": "^2.0.3"
  }
}
